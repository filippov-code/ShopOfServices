@model ICollection<Comment>
@{
    Layout = "_Layout";
}
<div class="container">
    @foreach (var comment in Model)
    {
        <div class="row border border-dark">
            <div class="col">
                <h3>@comment.SenderName</h3>
                <h3>@comment.Message</h3>
            </div>
        </div>
    }
</div>